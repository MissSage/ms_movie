{
  "name": "ms_movie",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress serve docs",
    "docs:deploy": "npm run docs:build & gh-pages -d docs/.vitepress/dist",
    "eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix --cache",
    "prettier": "prettier --write ./**/*.{html,vue,ts,cjs,json,md} --cache",
    "prepare": "husky install",
    "ict": "npm i -g commitizen",
    "format": "npm run eslint && npm run prettier",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm run install",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@three-ts/orbit-controls": "^1.4.7",
    "@turf/turf": "^6.5.0",
    "@videojs-player/vue": "^1.0.0",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.4.0",
    "cannon-es": "^0.20.0",
    "cesium": "1.106.1",
    "cesium-navigation-es6": "^3.0.8",
    "css-doodle": "^0.34.9",
    "d3": "^7.8.5",
    "dat.gui": "^0.7.9",
    "element-plus": "^2.3.7",
    "esri-leaflet": "^3.0.10",
    "esri-loader": "^3.7.0",
    "exceljs": "^4.3.0",
    "fancy-components": "^0.0.23",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.1",
    "leaflet": "^1.9.4",
    "leaflet.chinatmsproviders": "^3.0.6",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "three": "^0.151.3",
    "video.js": "^7.21.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/arcgis-js-api": "^4.27.0",
    "@types/d3": "^7.4.0",
    "@types/dat.gui": "^0.7.10",
    "@types/esri-leaflet": "^2.1.9",
    "@types/file-saver": "^2.0.5",
    "@types/leaflet": "^1.9.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.18",
    "@types/nprogress": "^0.2.0",
    "@types/three": "^0.150.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitepress-demo-preview/component": "^2.3.0",
    "@vitepress-demo-preview/plugin": "^1.1.12",
    "cz-git": "^1.6.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "sharp": "^0.32.1",
    "svgo": "^3.0.2",
    "typescript": "^5.1.5",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^1.1.6",
    "vitepress": "1.0.0-beta.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.2"
  }
}
